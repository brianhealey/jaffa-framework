        <field name="IPAddress" use-member="m_iPAddress" type="java.lang.String">
            <sql name="IP_ADDRESS" type="STRING" dirty="ignore"/>
        </field>

        <field name="Port" use-member="m_port" type="java.lang.String">
            <sql name="PORT" type="STRING" dirty="ignore"/>
        </field>

        <field name="StartTime" use-member="m_startTime" type="org.jaffa.datatypes.DateTime">
            <sql name="START_TIME" type="DATE_TIME" dirty="ignore"/>
        </field>

        <field name="StopTime" use-member="m_stopTime" type="org.jaffa.datatypes.DateTime">
            <sql name="STOP_TIME" type="DATE_TIME" dirty="ignore"/>
