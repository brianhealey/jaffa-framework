        <field name="MaxProcessingTime" use-member="m_maxProcessingTime" type="java.lang.String">
            <sql name="MAX_PROCESSING_TIME" type="STRING" dirty="ignore"/>
        </field>

        <field name="AvgProcessingTime" use-member="m_avgProcessingTime" type="java.lang.String">
            <sql name="AVG_PROCESSING_TIME" type="STRING" dirty="ignore"/>
        </field>

        <field name="CreatedOn" use-member="m_createdOn" type="org.jaffa.datatypes.DateTime">
