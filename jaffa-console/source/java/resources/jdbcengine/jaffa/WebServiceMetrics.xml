<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE classmap PUBLIC "-//JAFFA//DTD JDBC Engine Mapping 1.0//EN" "http://jaffa.sourceforge.net/DTD/jdbc-engine-mapping_1_0.dtd">

<classmap>
    <class name="org.jaffa.wsapi.domain.WebServiceMetrics">
        <map-to table="J_WEBSERVICE_METRICS" />

        <field name="Id" use-member="m_id" type="java.lang.String" primary-key="true">
            <sql name="ID" type="STRING" dirty="ignore"/>
        </field>

        <field name="EndpointName" use-member="m_endpointName" type="java.lang.String">
            <sql name="ENDPOINT_NAME" type="STRING" dirty="ignore"/>
        </field>

        <field name="EndpointAddress" use-member="m_endpointAddress" type="java.lang.String">
            <sql name="ENDPOINT_ADDRESS" type="STRING" dirty="ignore"/>
